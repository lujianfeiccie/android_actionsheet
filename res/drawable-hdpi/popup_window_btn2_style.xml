<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true">
  <shape>
      <gradient android:startColor="#cbcbcd" android:endColor="#cbcbcd" android:angle="90"/>
      <stroke android:width="1dip" android:color="#67a13b"/>
      <corners android:radius="5dp"/>
      <padding android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp"/>
  </shape>
  </item>
   <item android:state_focused="true">
  <shape>
      <gradient android:startColor="#2b2f38" android:endColor="#2b2f38" android:angle="90"/>
      <stroke android:width="1dip" android:color="#67a13b"/>
      <corners android:radius="5dp"/>
      <padding android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp"/>
  </shape>
  </item>
  <item>
  <shape>
      <gradient android:startColor="#2b2f38" android:endColor="#2b2f38" android:angle="90"/>
      <stroke android:width="1dip" android:color="#2b2f38"/>
      <corners android:radius="5dp"/>
      <padding android:left="5dp" android:top="5dp" android:right="5dp" android:bottom="5dp"/>
  </shape>
  </item>
</selector>
